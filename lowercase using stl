#include<bits/stdc++.h>
using namespace std;

string toLowercase(string str)
{
	transform(str.begin(), str.end(), str.begin(), ::tolower);
	return str;
}
int main() {
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#ifndef ONLINE_JUDGE
	freopen("input1.txt", "r", stdin);
	freopen("output1.txt", "w", stdout);
#endif
	string str;
	getline(cin , str);
	cout << toLowercase(str);

	return 0;
}
